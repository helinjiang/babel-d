{
  "name": "babel-d",
  "version": "0.1.1",
  "description": "compile directories by babel using in node",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir lib/ --source-maps",
    "eslint": "eslint src/",
    "prepublish": "npm run compile",
    "test": "npm run eslint && npm run test-cov",
    "test-babel": "mocha --compilers js:babel-core/register",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -t 50000 --recursive  -R spec test/",
    "watch": "npm run watch-compile",
    "watch-compile": "npm run compile -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helinjiang/babel-d.git"
  },
  "keywords": [
    "babel",
    "es6",
    "transform",
    "compile"
  ],
  "author": "helinjiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helinjiang/babel-d/issues"
  },
  "homepage": "https://github.com/helinjiang/babel-d#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "fs-extra": "^3.0.1",
    "lodash": "^4.17.4",
    "walk-sync": "^0.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^4.2.0",
    "mocha": "^3.4.2",
    "source-map": "^0.5.6"
  }
}
